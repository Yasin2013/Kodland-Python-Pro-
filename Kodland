meme_dict = {
            "Cringe": ""Garip ya da utandırıcı bir şey",
            "LOL": ""Komik bir şeye verilen cevap",
            "RUFL": "JOKE"
            }

word = input("Anlaşılmaz bir kelime girin (büyük harflerle!): ")

if word in meme_dict.keys():
    print(meme_dict[word])
else:
    print("Henüz bu kelimeye sahip değiliz... Ama biz bunun üzerinde çalışıyoruz!")
